  "dependencies": {
    "@babel/core": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "babel-jest": "^29.7.0",
    "child_process": "^1.0.2",
    "commander": "^12.1.0",
    "inquirer": "^9.2.23",
    "jest": "^29.7.0",
    "run-func": "^3.0.0",
    "xml2js": "^0.6.2",
    "yaml": "^2.4.3"
  },
  "type": "module",
  "presets": [
    "@babel/preset-env"
